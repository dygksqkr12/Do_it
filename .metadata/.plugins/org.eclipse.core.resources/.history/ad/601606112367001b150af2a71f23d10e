package chapter01;

/**
 * 세 값의 대소 관계 13종류의 모든 조합에 대해 중앙값을 구하여 출력하는 프로그램을 작성하세요
 *
 */
public class Q04 {

	public static void main(String[] args) {
		
	}
	
	public static int mid(int a, int b, int c, 
						int d, int e, int f, 
						int g, int h, int i, 
						int j, int k, int l, 
						int m) {
		if(a >= b) 
			if(b >= c)
				return b;
			else if(a <= c)
				return a;
			else 
				return c;
		else if(a > c)
			return a;
		else if(b > c)
			return c;
		else 
			return b;
		

		
		
	}

}
